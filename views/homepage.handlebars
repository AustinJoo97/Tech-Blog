<!-- Loop over each object in the projects array -->
{{#each blogPosts as |blogPost| }}
<div class="row mb-4 project">
  <div class="col-md-5">
    <h2>
      <a href="/blogPost/{{blogPost.id}}">{{blogPost.post_text}}</a>
    </h2>
    <p>Posted by {{blogPost.user_id}} on {{format_date blogPost.date_created}}</p>
  </div>
  <div class="col-md-7">
    <p>
      {{project.description}}
    </p>
  </div>
</div>
{{/each}}
